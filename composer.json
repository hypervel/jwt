{
    "name": "swooletw/hyperf-jwt",
    "description": "The Support package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "jwt",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "jwtors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.16",
        "hyperf/collection": "~3.1.0",
        "lcobucci/jwt": "^5.0",
        "psr/simple-cache": "^3.0",
        "hyperf/stringable": "~3.1.0",
        "ramsey/uuid": "^4.7",
        "swooletw/hyperf-cache": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\JWT\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\JWT\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
